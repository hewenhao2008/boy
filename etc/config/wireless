config wifi-device 'ra0'
        option type 'ralink'
        option mode '9'
        option txpower '100'
        option ht '20'
        option disabled '0'
        option channel 'auto'
        option hidessid '0'
        option macpolicy '0'

config wifi-iface 'guest'
	option device 'ra0'
	option ifname 'ra0'
	option network 'wlan'
	option mode 'ap'
	option ssid '@Wipark-Free'
	option encryption 'none'
	option key 'none'
	option wpa_group_rekey '0'
        option wpa_pair_rekey '0'
        option wpa_master_rekey '0'
